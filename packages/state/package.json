{
  "name": "@juan-utils/state",
  "version": "2.3.0",
  "main": "./dist/cjs/juan-utils-state.cjs.js",
  "module": "./dist/esm/juan-utils-state.esm.js",
  "browser": "./dist/umd/juan-utils-state.umd.js",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Jkierem/juan-utils"
  },
  "files": [
    "dist",
    "types"
  ],
  "types": "types/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "test": "rm -rf ./coverage && jest --coverage --verbose",
    "test:watch": "jest --verbose --watch",
    "prepublish": "yarn build"
  },
  "dependencies": {
    "@juan-utils/functions": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "jest": "^24.9.0",
    "rollup": "^1.30.0",
    "rollup-plugin-babel": "^4.3.3",
    "sinon": "^7.4.1"
  },
  "jest": {
    "verbose": true,
    "collectCoverageFrom": [
      "src/*/*.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/?(*.)test.{js,jsx}"
    ],
    "testEnvironment": "node",
    "transform": {
      ".js$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ]
  }
}
