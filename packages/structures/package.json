{
    "name": "@juan-utils/structures",
    "version": "2.0.0-alpha.0",
    "main": "./dist/index.js",
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Jkierem/juan-utils"
    },
    "scripts": {
        "build": "webpack --mode production --config webpack.config.js",
        "test": "rm -rf ./coverage && jest --coverage --verbose",
        "test:watch": "jest --verbose --watch",
        "prepublish": "yarn build"
    },
    "dependencies": {
        "@juan-utils/functions": "^2.0.0-alpha.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.5.5",
        "@babel/core": "^7.5.5",
        "@babel/preset-env": "^7.5.5",
        "babel-jest": "^24.9.0",
        "babel-loader": "^8.0.6",
        "jest": "^24.9.0",
        "sinon": "^7.4.1",
        "webpack": "^4.39.3",
        "webpack-cli": "^3.3.7",
        "webpack-dev-server": "^3.8.0"
    },
    "babel": {
        "presets": [
            "@babel/env"
        ]
    },
    "jest": {
        "verbose": true,
        "collectCoverageFrom": [
            "src/*.{js,jsx}"
        ],
        "testMatch": [
            "<rootDir>/src/**/?(*.)test.{js,jsx}"
        ],
        "testEnvironment": "node",
        "transform": {
            ".js$": "babel-jest"
        },
        "transformIgnorePatterns": [
            "<rootDir>/node_modules/"
        ],
        "moduleDirectories": [
            "node_modules",
            "modules"
        ],
        "moduleFileExtensions": [
            "js",
            "json",
            "jsx"
        ]
    },
    "gitHead": "06279d689834978d68839e77413dc1edd6308f92"
}
