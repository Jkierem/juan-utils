{
    "name": "@juan-utils/structures",
    "version": "2.5.1",
    "main": "./dist/cjs/juan-utils-structs.cjs.js",
    "module": "./dist/esm/juan-utils-structs.esm.js",
    "browser": "./dist/umd/juan-utils-structs.umd.js",
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Jkierem/juan-utils"
    },
    "files": [
        "dist",
        "types"
    ],
    "scripts": {
        "build": "rollup -c",
        "test": "rm -rf ./coverage && jest --coverage --verbose",
        "test:watch": "jest --verbose --watch",
        "prepublish": "yarn build"
    },
    "dependencies": {
        "@juan-utils/functions": "^2.5.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.5.5",
        "@babel/core": "^7.5.5",
        "@babel/preset-env": "^7.5.5",
        "@rollup/plugin-node-resolve": "^7.0.0",
        "babel-jest": "^24.9.0",
        "babel-loader": "^8.0.6",
        "babel-polyfill": "^6.26.0",
        "jest": "^24.9.0",
        "rollup": "^1.30.0",
        "rollup-plugin-babel": "^4.3.3",
        "sinon": "^7.4.1",
        "webpack": "^4.39.3",
        "webpack-cli": "^3.3.7",
        "webpack-dev-server": "^3.8.0"
    },
    "jest": {
        "verbose": true,
        "collectCoverageFrom": [
            "src/*/*.{js,jsx}",
            "src/*/*/*.{js,jsx}"
        ],
        "testMatch": [
            "<rootDir>/src/**/?(*.)test.{js,jsx}"
        ],
        "testEnvironment": "node",
        "transform": {
            ".js$": "babel-jest"
        },
        "transformIgnorePatterns": [
            "<rootDir>/node_modules/"
        ],
        "moduleDirectories": [
            "node_modules",
            "modules"
        ],
        "moduleFileExtensions": [
            "js",
            "json",
            "jsx"
        ],
        "setupFilesAfterEnv": [
            "./setupTests.js"
        ]
    },
    "gitHead": "06279d689834978d68839e77413dc1edd6308f92"
}
